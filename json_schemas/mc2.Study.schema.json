        "enum": [
          "RS",
          "NPU",
          "PS",
          "GSO",
          "HMB",
          "NPUNCU",
          "DS",
          "TS",
          "CC",
          "POA",
          "US",
          "IRB",
          "MOR",
          "DUM",
          "GRU",
          "IS",
          "RTN",
          "NMDS",
          "PUB",
          "NCU",
          "GS",
          "NPOA",
          "COL",
          "NRES",
          ""
        ]
      },
