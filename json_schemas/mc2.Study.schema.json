      "not": {
        "type": "null"
      },
      "minLength": 1
    },
    "Study License": {
      "enum": [
        "CC BY-NC 3.0",
        "CC BY 4.0",
        "CC BY 3.0",
        "CC BY-NC 4.0",
        ""
      ]
